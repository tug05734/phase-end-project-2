<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>UserAuthentication</servlet-name>
  	<display-name>UserAuthentication</display-name>
  	<description></description>
  	<servlet-class>com.servlets.UserAuthentication</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UserRegistration</servlet-name>
  	<display-name>UserRegistration</display-name>
  	<description></description>
  	<servlet-class>com.servlets.UserRegistration</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>UserAuthentication</servlet-name>
  	<url-pattern>/userAuth</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UserRegistration</servlet-name>
  	<url-pattern>/userReg</url-pattern>
  </servlet-mapping>
</web-app>